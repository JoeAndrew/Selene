#!./Selene

-- Collection example

col = SelTimedCollection.create(5)

for i=1,4 do
	col:Push(i, os.time()+i)
end
col:dump()

for i=5,7 do
	col:Push(i)
end
col:dump()

for i=1,5 do
	col:Push( math.random(0,1000), os.time() + math.random(0,1000000) - 500000 )
end
col:dump()

print( "MinMax", col:MinMax() )

for d,t in col:iData() do print(d, os.date("%x",t) ) end
